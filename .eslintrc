{
  "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warn", { "singleQuote": true, "endOfLine": "auto" }],
    "react-hooks/exhaustive-deps": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint"
}
